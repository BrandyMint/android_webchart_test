<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.brandymint.androidchart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

	<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:installLocation="auto"
        android:hardwareAccelerated="true"
        >
        <activity
            android:name="ru.brandymint.androidchart.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
		<activity
            android:name="ru.brandymint.androidchart.achartengine.AChartEngineActivity"
            android:label="@string/achartengine_title" >
        </activity>
        
		<activity
            android:name="ru.brandymint.androidchart.webview.WebView1Activity"
            android:label="@string/webview1_title" >
        </activity>
        
		<activity
            android:name="ru.brandymint.androidchart.keepedge.KeepEdgeActivity"
            android:label="@string/keepedge_title" >
        </activity>
        
		<activity
            android:name="ru.brandymint.androidchart.teechart.TeeChartActivity"
            android:label="@string/teechart_title" >
        </activity>
        
    </application>

</manifest>